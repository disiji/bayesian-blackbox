python bayesian_reliability_comparison.py dbpedia -pseudocount 1 -ground_truth_type frequentist -weight_type pool &
python bayesian_reliability_comparison.py cifar100 -pseudocount 1 -ground_truth_type bayesian -weight_type pool &
